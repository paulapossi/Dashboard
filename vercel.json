{
  "buildCommand": "cd 02_frontend && npm run build",
  "outputDirectory": "02_frontend/.next",
  "installCommand": "cd 02_frontend && npm install",
  "framework": "nextjs",
  "functions": {
    "maxDuration": 10
  },
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/public/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ]
}

